{
  "name": "hcbs-revenue-management-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.6.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "ts-node src/scripts/build.ts",
    "test": "jest",
    "test:unit": "jest --projects=unit",
    "test:integration": "jest --projects=integration",
    "test:e2e": "jest --projects=e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "migrate": "ts-node src/scripts/migration.ts run",
    "migrate:rollback": "ts-node src/scripts/migration.ts rollback",
    "migrate:list": "ts-node src/scripts/migration.ts list",
    "migrate:create": "ts-node src/scripts/migration.ts create",
    "seed": "ts-node src/scripts/seed.ts",
    "seed:dev": "ts-node src/scripts/seed.ts --env development",
    "seed:test": "ts-node src/scripts/seed.ts --env test",
    "docs": "ts-node src/scripts/generate-api-docs.ts",
    "prepare": "cd .. && husky install backend/.husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.350.0",
